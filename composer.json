{
    "name": "scalexy/nova-image-cropper",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ScaleXY\\NovaImageCropper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScaleXY\\NovaImageCropper\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}